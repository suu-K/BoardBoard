<div class="container px-lg-5 text-center">
    <table class="table" style="padding-left: 10%;padding-right: 10%;">
        <thead>
            <tr>
                <th style>번호</th>
                <th style>제목</th>
                <th style>작성일</th>
            </tr>
        </thead>
        <tbody>

            <%if (posts) {%>
                <% for(var post in posts){ %>
                    <tr>
                        <th><%=post.id%></th>
                        <th><%=post.title%></th>
                        <th><%=post.createdate%></th>
                </tr>
                <%}%>
            <%}%>
        </tbody>
    </table>
</div>
<div class="container px-lg-5">
    <%if(1) {%>
    <button class="btn bg-light" onclick="location.href='party/writeNotice'" style="position: relative; left: 92%;">공지 작성</button>
    <%}%>
</div>






